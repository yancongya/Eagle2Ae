{
  "name": "copypasta",
  "version": "1.0.4",
  "description": "An After Effects plugin for clipboard management.",
  "extendScriptClass": "Clippy",
  "manifest": {
    "bundleName": "Copy Pasta",
    "bundleId": "com.aescripts.copypasta",
    "hosts": {
      "AEFT": "13.0"
    },
    "requiredRuntimes": {
      "CSXS": "5.0"
    },
    "extensions": {
      "panel": {
        "title": "Copy Pasta",
        "size": [
          200,
          200
        ]
      },
      "about": {
        "type": "Modeless",
        "size": [
          350,
          440
        ],
        "maxSize": [
          350,
          500
        ],
        "minSize": [
          350,
          100
        ],
        "manifestOnly": true,
        "showMenu": false,
        "mainPath": "./dialog/dialog.html",
        "scriptPath": null
      }
    }
  },
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "neutrino build",
    "start": "neutrino start",
    "lint": "neutrino lint",
    "test": "neutrino test",
    "release": "standard-version -r",
    "deploy-local": "cross-env JSX_DEBUG=true node ./scripts/deploy-local.js",
    "package": "node ./scripts/package.js",
    "publish-release": "node ./scripts/release.js"
  },
  "author": "Sam Marks <sam@sammarks.me>",
  "devDependencies": {
    "@neutrinojs/jest": "^8.3.0",
    "@neutrinojs/standardjs": "^8.3.0",
    "@pluginplay/cep-scripts": "0.0.6",
    "@sentry/webpack-plugin": "^1.5.2",
    "cep-lib": "0.0.6",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-enzyme": "^1.0.0-beta.1",
    "chai-jest-diff": "^1.0.2",
    "cross-env": "^5.2.0",
    "dirty-chai": "^2.0.1",
    "enzyme": "^3.4.2",
    "enzyme-adapter-react-16": "^1.2.0",
    "jest": "^23.5.0",
    "jest-junit": "^5.1.0",
    "jest-styled-components": "^6.0.1",
    "localstorage-polyfill": "^1.0.1",
    "lolex": "^2.7.1",
    "md-2-json": "^1.0.5",
    "mobx": "^4.4.0",
    "mobx-react": "^5.2.6",
    "mock-spawn": "^0.2.6",
    "moment": "^2.22.2",
    "neutrino": "^8.3.0",
    "neutrino-preset-cep": "^0.0.17",
    "node-github-release": "^0.2.1",
    "prop-types": "^15.6.2",
    "raf": "^3.4.0",
    "raven-js": "^3.26.4",
    "react": "^16.4.1",
    "react-cep": "0.0.6",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-test-renderer": "^16.4.2",
    "standard-version": "^4.4.0",
    "styled-components": "^3.3.3",
    "styled-flex-component": "^2.2.2",
    "uuid": "^3.3.2"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "mkdirp": "^0.5.1",
    "path": "^0.12.7",
    "temp": "^0.8.3"
  }
}
